{
  "name": "estream-app",
  "version": "0.3.0",
  "private": true,
  "description": "eStream Mobile App - React Native client for the eStream network",
  "repository": {
    "type": "git",
    "url": "https://github.com/toddrooke/estream-app.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:watch": "jest --config jest.e2e.config.js --watch",
    "test:e2e:runner": "./scripts/run-e2e-tests.sh",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "cd android && ./gradlew clean && cd ..",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "android:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:build": "cd android && ./gradlew assembleDebug && cd ..",
    "android:logs": "adb logcat -s EstreamApp:V"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.1.1",
    "@estream/react-native": "file:../estream/packages/mobile-sdk/react-native",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@noble/hashes": "^2.0.1",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-camera-roll/camera-roll": "^7.8.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.5",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.5",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.34.0",
    "react-native-share": "^10.2.0",
    "react-native-svg": "^14.1.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-view-shot": "^3.8.0",
    "react-native-vision-camera": "^4.0.5",
    "text-encoding": "^0.7.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
